{
    "servers": {
        "example": {
            "path"   : "/absolute/path/to/server/root",
            "launch" : "java -server -Xincgc -Xmx1G -jar CraftBukkit.jar",
            "timeout": 60,
            
            "packages": [
                "AllowPlayers",   "CraftBukkit",    "CraftIRC"     "dynmap",
                "Honeypot",       "LogBlock",       "LWC",         "mcMMO",
                "Minequery",      "MobDisguise",    "NoCheat",     "PermissionsCompat",
                "PermissionsEx",  "PhysicalShop",   "SpamHammer",  "Spout",
                "Transporter",    "VanishNoPacket", "WorldEdit",   "WorldGuard"
            ],
            
            "archives": {
                "log": {
                    "path"        : "/absolute/path/to/server/root/logs",
                    "max-size"    : 20,
                    "max-archives": 0,
                    "paths"       : ["server.log"]
                },
                
                "backup": {
                    "path"        : "/absolute/path/to/server/root/backup",
                    "max-size"    : 0,
                    "max-archives": 30,
                    "paths"       : ["plugins", "world", "world_nether"]
                }
            }
        }
    },
    
    "packages": {
        "AllowPlayers": {
            "path"   : "plugins",
            "type"   : "jar",
            "url"    : "http://dev.bukkit.org/server-mods/allowplayers/",
            "updater": "bukkitdev",
            "extract": []
        },
        
        "CraftBukkit": {
            "path"   : null,
            "type"   : "jar",
            "url"    : "http://dl.bukkit.org/downloads/craftbukkit/list/dev/",
